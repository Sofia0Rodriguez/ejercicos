def esmultiplo (numero, divisor):
    return numero % divisor == 0

def respuesta (numero, divisor):
   if esmultiplo(numero, divisor): 
       return f"{numero} es multiplo de {divisor}"  
   else: 
     return f"{numero} NO es multiplo de {divisor}"

numero = 1536948
divisores=[2, 3, 5, 7, 9, 10, 11]
    
   
for divisor in divisores: 
    print(respuesta(numero, divisor))
    

